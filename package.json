{
  "name": "nest-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tscx -rc",
    "dev": "tscx -rcw -e dist/main.js",
    "postinstall": "git-validator install",
    "style": "git-validator",
    "style:update": "git-validator -u"
  },
  "dependencies": {
    "@nestjs/common": "10.3.8",
    "@nestjs/core": "10.3.8",
    "@nestjs/platform-express": "10.3.8",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@rnm/tscx": "0.1.1",
    "@types/node": "20.12.7",
    "git-validator": "0.16.0",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  }
}
