{
  "name": "nest-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tscx -rc",
    "dev": "tscx -rcw -e dist/main.js",
    "postinstall": "git-validator install",
    "style": "git-validator",
    "style:update": "git-validator -u"
  },
  "dependencies": {
    "@nestjs/common": "10.3.10",
    "@nestjs/core": "10.3.10",
    "@nestjs/platform-express": "10.3.10",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@rnm/tscx": "0.1.4",
    "@types/node": "22.1.0",
    "git-validator": "0.18.9",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=18"
  }
}
